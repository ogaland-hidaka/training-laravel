du:
	docker-compose up -d nginx
de:
	docker-compose exec -u root work bash
due:
	docker-compose up -d nginx
	docker-compose exec -u root work bash
dd:
	docker-compose down

du2:
	docker compose up -d nginx
de2:
	docker compose exec -u root work bash
due2:
	docker compose up -d nginx
	docker compose exec -u root work bash
dd2:
	docker compose down